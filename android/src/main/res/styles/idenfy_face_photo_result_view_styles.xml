<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="idenfyFacePhotoResultViewTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyFacePhotoResultViewTitleTextColor</item>
        <item name="android:textSize">
            @dimen/idenfy_dimen_face_photo_results_view_information_title_text_size
        </item>
    </style>

    <style name="idenfyFacePhotoResultViewDescriptionStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyFacePhotoResultViewDescriptionTextColor</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="idenfyFacePhotoResultViewRetakePhotoButtonTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyFacePhotoResultViewRetakePhotoButtonTextColor
        </item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyFacePhotoResultViewNextButtonStyle" parent="android:Widget.Button">
        <item name="android:textColor">@color/idenfyFacePhotoResultViewNextButtonTextColor</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyFacePhotoResultViewDetailCardTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyFacePhotoResultViewCardTitleColor</item>
        <item name="android:textSize">12sp</item>

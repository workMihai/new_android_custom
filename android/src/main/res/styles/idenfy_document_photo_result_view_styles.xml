<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="idenfyDocumentPhotoResultViewTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyDocumentPhotoResultViewTitleTextColor</item>
        <item name="android:textSize">
            @dimen/idenfy_dimen_document_photo_results_view_information_title_text_size
        </item>
    </style>

    <style name="idenfyDocumentPhotoResultViewDescriptionStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyDocumentPhotoResultViewDescriptionTextColor
        </item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="idenfyDocumentPhotoResultViewRetakePhotoButtonTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">
            @color/idenfyDocumentPhotoResultViewRetakePhotoButtonTextColor
        </item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyDocumentPhotoResultViewNextButtonStyle" parent="android:Widget.Button">
        <item name="android:textColor">@color/idenfyDocumentPhotoResultViewNextButtonTextColor
        </item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyDocumentPhotoResultViewDetailCardTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyDocumentPhotoResultViewCardTitleColor</item>
        <item name="android:textSize">12sp</item>
    </style>

</resources>
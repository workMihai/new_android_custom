<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="idenfyUploadPhotoViewTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyUploadPhotoViewTitleTextColor</item>
        <item name="android:textSize">
            @dimen/idenfy_dimen_upload_photo_view_information_title_text_size
        </item>
    </style>

    <style name="idenfyUploadPhotoViewDescriptionStyle" parent="android:Widget.TextView">
        <item name="android:textColor">@color/idenfyUploadPhotoViewDescriptionTextColor</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="idenfyUploadPhotoViewChooseAnotherPhotoButtonTitleStyle" parent="android:Widget.TextView">
        <item name="android:textColor">
            @color/idenfyUploadPhotoViewChooseAnotherPhotoButtonTextColor
        </item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyUploadPhotoViewContinuePhotoButtonStyle" parent="android:Widget.Button">
        <item name="android:textColor">@color/idenfyUploadPhotoViewContinuePhotoButtonTextColor
        </item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="idenfyUploadPhotoViewBackButtonStyle">
        <item name="android:tint">
            @color/idenfyMainColorV2
        </item>
    </style>

</resources>